<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="https://jakarta.ee/xml/ns/jakartaee" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_5_0.xsd" id="WebApp_ID" version="5.0">
<servlet>
<servlet-name>login</servlet-name>
<servlet-class>com.unique.LoginServlet</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>login</servlet-name>
<url-pattern>/login</url-pattern>
</servlet-mapping>


<servlet>
    <servlet-name>HomePageServlet</servlet-name>
    <servlet-class>com.unique.HomePage</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>HomePageServlet</servlet-name>
    <url-pattern>/homepage</url-pattern>
</servlet-mapping>


<servlet>
    <servlet-name>ActiveComplaints</servlet-name>
    <servlet-class>com.unique.ActiveComplaints</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>ActiveComplaints</servlet-name>
    <url-pattern>/activeComplaints</url-pattern>
</servlet-mapping>


<servlet>
    <servlet-name>Logout</servlet-name>
    <servlet-class>com.unique.LogoutServlet</servlet-class>
</servlet>
<servlet-mapping>
    <servlet-name>Logout</servlet-name>
    <url-pattern>/logout</url-pattern>
</servlet-mapping>
<servlet>
    <servlet-name>TakeAction</servlet-name>
    <servlet-class>com.unique.TakeActions</servlet-class>
</servlet>

<servlet-mapping>
    <servlet-name>TakeAction</servlet-name>
    <url-pattern>/takeAction</url-pattern>
</servlet-mapping>

 <filter>
    <display-name>Filter</display-name>
    <filter-name>Filter</filter-name>
    <filter-class>com.unique.Filter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>Filter</filter-name>
    <url-pattern>/login</url-pattern>
  </filter-mapping>

</web-app>